<div id="leads">
    <table class="table table-striped">
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
        @foreach (var lead in Model)
        {
            <tr>
                <td>@lead.FirstName</td>
                <td>@lead.LastName</td>
                <td>@lead.Email</td>
            </tr>
        }
    </table>
</div>